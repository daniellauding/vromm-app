  "scripts": {
    "start": "node --max-old-space-size=12288 --optimize-for-size --gc-interval=100 --max-semi-space-size=128 node_modules/.bin/expo start",
    "start-clean": "node --max-old-space-size=12288 --optimize-for-size --gc-interval=100 --max-semi-space-size=128 node_modules/.bin/expo start --clear",
    "start-jsc": "EXPO_USE_JSC=1 node --max-old-space-size=12288 --optimize-for-size --gc-interval=100 --max-semi-space-size=128 node_modules/.bin/expo start --clear",
    "start-memory": "node --max-old-space-size=12288 --optimize-for-size --gc-interval=100 node_modules/.bin/expo start",
    "start-monitor": "node scripts/memory-monitor.js",
    "start-protected": "node scripts/start-with-memory-protection.js",
    "clear-caches": "node scripts/clear-caches.js",
    "dev": "node scripts/start-dev-server.js",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "setup-translations": "node ./scripts/apply-translations-migration.js",
    "manual-setup-translations": "node ./scripts/manual-apply-translations.js",
    "cleanup-migrations": "node ./scripts/cleanup-translations-migrations.js",
